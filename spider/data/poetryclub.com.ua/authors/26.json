    "poems": [
        {
            "name": "\"І вдень журюсь, і серед ночі...\"",
            "link": "metrs_poem.php?poem=9011"
        },
        {
            "name": "\"Благословляю час той, і годину...\"",
            "link": "metrs_poem.php?poem=8733"
        },
        {
            "name": "\"Блукав я самітно проміж деревами...\"",
            "link": "metrs_poem.php?poem=9031"
        },
        {
            "name": "\"Блукаю між квітками...\"",
            "link": "metrs_poem.php?poem=9028"
        },
        {
            "name": "\"В твої голубенькі очі...\"",
            "link": "metrs_poem.php?poem=9024"
        },
        {
            "name": "\"Вітаю глуш твою, куточку мій затишний...\"",
            "link": "metrs_poem.php?poem=8709"
        },
        {
            "name": "\"Весняні блакитні очі...\"",
            "link": "metrs_poem.php?poem=9013"
        },
        {
            "name": "\"Вранці я встаю й питаю...\"",
            "link": "metrs_poem.php?poem=9030"
        },
        {
            "name": "\"Заплачеш по мені, кохана, о! заплачеш...\"",
            "link": "metrs_poem.php?poem=8877"
        },
        {
            "name": "\"Знов я серце упокорив...\"",
            "link": "metrs_poem.php?poem=9025"
        },
        {
            "name": "\"Квітки з сльозами...\"",
            "link": "metrs_poem.php?poem=8723"
        },
        {
            "name": "\"Коли б ти, кохана, мені притулила до серденька руку...\"",
            "link": "metrs_poem.php?poem=9032"
        },
        {
            "name": "\"Коли б я був хлоп'я байдужне без турботи...\"",
            "link": "metrs_poem.php?poem=8985"
        },
        {
            "name": "\"Коли добрі очі маєш...\"",
            "link": "metrs_poem.php?poem=9014"
        },
        {
            "name": "\"Колись про любощі гарячі я мечтав...\"",
            "link": "metrs_poem.php?poem=8899"
        },
        {
            "name": "\"Любить метелик пишну троянду...\"",
            "link": "metrs_poem.php?poem=8930"
        },
        {
            "name": "\"Люблю я квітку, да котру, не знаю...\"",
            "link": "metrs_poem.php?poem=8927"
        },
        {
            "name": "\"МЕНШИЙ БРАТ\"",
            "link": "metrs_poem.php?poem=8635"
        },
        {
            "name": "\"НАРОДНЫЙ ДОМ\"",
            "link": "metrs_poem.php?poem=8694"
        },
        {
            "name": "\"На добраніч усім на ніч...\"",
            "link": "metrs_poem.php?poem=8520"
        },
        {
            "name": "\"Ох! бажаю сліз любові...\"",
            "link": "metrs_poem.php?poem=9012"
        },
        {
            "name": "\"Предивний сон і звеселив...\"",
            "link": "metrs_poem.php?poem=8865"
        },
        {
            "name": "\"Прийшов май до нас у гості...\"",
            "link": "metrs_poem.php?poem=8928"
        },
        {
            "name": "\"Розвивається і зеленіє...\"",
            "link": "metrs_poem.php?poem=8897"
        },
        {
            "name": "\"Серце моє тихе...\"",
            "link": "metrs_poem.php?poem=8722"
        },
        {
            "name": "\"Старий гай мрій, пахущий гай...\"",
            "link": "metrs_poem.php?poem=8775"
        },
        {
            "name": "\"Стиха серце підмиває...\"",
            "link": "metrs_poem.php?poem=8929"
        },
        {
            "name": "\"Стоїть ніч темна весняна...\"",
            "link": "metrs_poem.php?poem=8950"
        },
        {
            "name": "\"Ти сидиш під білим древом...\"",
            "link": "metrs_poem.php?poem=8896"
        },
        {
            "name": "\"Тим від тебе утікаю...\"",
            "link": "metrs_poem.php?poem=9027"
        },
        {
            "name": "\"Тихий берег понад морем...\"",
            "link": "metrs_poem.php?poem=9023"
        },
        {
            "name": "\"Троянда запашна... та чи вона ж то чує...\"",
            "link": "metrs_poem.php?poem=9026"
        },
        {
            "name": "\"Чого кипить химерна кров...\"",
            "link": "metrs_poem.php?poem=8878"
        },
        {
            "name": "\"Чудовні очі весняної ночі...\"",
            "link": "metrs_poem.php?poem=8898"
        },
        {
            "name": "\"Що в мені ти закохалась...\"",
            "link": "metrs_poem.php?poem=9022"
        },
        {
            "name": "\"Що тебе носить кругом серед сонної літньої ночі...\"",
            "link": "metrs_poem.php?poem=9015"
        },
        {
            "name": "\"Я бачив сон: панок низенький чепурненький...\"",
            "link": "metrs_poem.php?poem=9029"
        },
        {
            "name": "\"Як увечері блукаю...\"",
            "link": "metrs_poem.php?poem=9021"
