    "poems": [
        {
            "name": "\"А що в Москві? Панують знову білі...\"",
            "link": "metrs_poem.php?poem=2206"
        },
        {
            "name": "\"Акації. Бджолині дзвони...\"",
            "link": "metrs_poem.php?poem=10987"
        },
        {
            "name": "\"В гуцульській хаті на печі...\"",
            "link": "metrs_poem.php?poem=3292"
        },
        {
            "name": "\"Візьми мене, як арфу, поміж ноги...\"",
            "link": "metrs_poem.php?poem=10991"
        },
        {
            "name": "\"Вночі ти входила в море...\"",
            "link": "metrs_poem.php?poem=10981"
        },
        {
            "name": "\"Все не те, коли нема любові...\"",
            "link": "metrs_poem.php?poem=3293"
        },
        {
            "name": "\"Дівочих непорочних ліній...\"",
            "link": "metrs_poem.php?poem=10984"
        },
        {
            "name": "\"Згадав я Чигирин. Моя печале...\"",
            "link": "metrs_poem.php?poem=2210"
        },
        {
            "name": "\"Злітає сукня і сорочка...\"",
            "link": "metrs_poem.php?poem=11005"
        },
        {
            "name": "\"Коли ми йшли удвох з тобою...\"",
            "link": "metrs_poem.php?poem=3295"
        },
        {
            "name": "\"Коли помер кривавий Торквемада...\"",
            "link": "metrs_poem.php?poem=287"
        },
        {
            "name": "\"ЛЮБІ ДРУЗІ\"",
            "link": "metrs_poem.php?poem=7657"
        },
        {
            "name": "\"Між персами твоїми - жолобок...\"",
            "link": "metrs_poem.php?poem=10992"
        },
        {
            "name": "\"Мені твоїх поцілунків...\"",
            "link": "metrs_poem.php?poem=10999"
        },
        {
            "name": "\"Ми вбогими були й жили в печалі...\"",
            "link": "metrs_poem.php?poem=2209"
        },
        {
            "name": "\"Ми такі холодні...\"",
            "link": "metrs_poem.php?poem=11011"
        },
        {
            "name": "\"Мов блискучі рукояті...\"",
            "link": "metrs_poem.php?poem=11012"
        },
        {
            "name": "\"Молюся й думаю про людські біди...\"",
            "link": "metrs_poem.php?poem=2208"
        },
        {
            "name": "\"Море з моря ткалось гладко...\"",
            "link": "metrs_poem.php?poem=10985"
        },
        {
            "name": "\"Моя гріховнице пречиста...\"",
            "link": "metrs_poem.php?poem=3297"
        },
        {
            "name": "\"Мріє, наче сніг здалека...\"",
            "link": "metrs_poem.php?poem=10980"
        },
        {
            "name": "\"На Лесбосі ніколи не була ти...\"",
            "link": "metrs_poem.php?poem=11002"
        },
        {
            "name": "\"Найдовша з усіх доріг...\"",
            "link": "metrs_poem.php?poem=3298"
        },
        {
            "name": "\"Найліпше любитись в надрання...\"",
            "link": "metrs_poem.php?poem=11007"
        },
        {
            "name": "\"Напав я на тебе раптом...\"",
            "link": "metrs_poem.php?poem=10986"
        },
        {
            "name": "\"О рідне слово, хто без тебе я?..\"",
            "link": "metrs_poem.php?poem=3299"
        },
        {
            "name": "\"Повітря нічне до рання...\"",
            "link": "metrs_poem.php?poem=10988"
        },
        {
            "name": "\"Посеред ночі дощ пішов...\"",
            "link": "metrs_poem.php?poem=10989"
        },
        {
            "name": "\"Скинь одежу свою...\"",
            "link": "metrs_poem.php?poem=10982"
        },
        {
            "name": "\"Спідниця, втята на півслові...\"",
            "link": "metrs_poem.php?poem=10997"
        },
        {
            "name": "\"Спадала вниз оголена вода...\"",
            "link": "metrs_poem.php?poem=10996"
        },
        {
            "name": "\"Так, ти одна, моя любове...\"",
            "link": "metrs_poem.php?poem=3301"
        },
        {
            "name": "\"Твої груди, наче свічі...\"",
            "link": "metrs_poem.php?poem=10998"
        },
        {
            "name": "\"Ти виходила з моря гола...\"",
            "link": "metrs_poem.php?poem=11000"
        },
        {
            "name": "\"Ти дуже гарна, але я з тобою...\"",
            "link": "metrs_poem.php?poem=10994"
        },
        {
            "name": "\"Ти зіграла двісті ролей...\"",
            "link": "metrs_poem.php?poem=11008"
        },
        {
            "name": "\"Ти пахнеш, наче конюшина біла...\"",
            "link": "metrs_poem.php?poem=11003"
        },
        {
            "name": "\"Ти, мов яблуко з галузки...\"",
            "link": "metrs_poem.php?poem=10990"
        },
        {
            "name": "\"У дверях на летовищі стояла...\"",
            "link": "metrs_poem.php?poem=11001"
        },
        {
            "name": "\"Хочеш бути, як монашка...\"",
            "link": "metrs_poem.php?poem=11014"
        },
        {
            "name": "\"Хочу тебе цілувати. Рятунок...\"",
            "link": "metrs_poem.php?poem=10983"
        },
        {
            "name": "\"Червоні яблука в кімнатах...\"",
            "link": "metrs_poem.php?poem=10978"
        },
        {
            "name": "\"Я буду витрачати...\"",
            "link": "metrs_poem.php?poem=10995"
        },
        {
            "name": "\"Я в пазуху зайду тайком...\"",
            "link": "metrs_poem.php?poem=11004"
        },
        {
            "name": "\"Я вам прощаю все, брати-москвини...\"",
            "link": "metrs_poem.php?poem=2207"
        },
        {
            "name": "\"Я вчився сорок літ, та не навчився досі...\"",
            "link": "metrs_poem.php?poem=11009"
        },
        {
            "name": "\"Я діяв напролом...\"",
            "link": "metrs_poem.php?poem=11015"
        },
        {
            "name": "\"Я за тобою навздогінці...\"",
            "link": "metrs_poem.php?poem=11010"
        },
        {
            "name": "\"Я пригадав собі один стіжок у горах...\"",
            "link": "metrs_poem.php?poem=10979"
        },
        {
            "name": "\"Я роздягнув тебе в уяві...\"",
            "link": "metrs_poem.php?poem=11006"
        },
        {
            "name": "\"Я стужився, мила, за тобою...\"",
            "link": "metrs_poem.php?poem=3302"
        },
        {
            "name": "\"Як ти лежала горілиць...\"",
            "link": "metrs_poem.php?poem=11013"
        },
        {
            "name": "\"Якби я знав, де ти живеш...\"",
            "link": "metrs_poem.php?poem=10993"
        },
