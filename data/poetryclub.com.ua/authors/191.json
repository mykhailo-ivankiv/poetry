    "poems": [
        {
            "name": "\"...звільна плавно від волосся...\"",
            "link": "metrs_poem.php?poem=11169"
        },
        {
            "name": "\"...твій вигин плеча брови бедра...\"",
            "link": "metrs_poem.php?poem=11164"
        },
        {
            "name": "\"Вітер відкотиться каменем чи кураєм...\"",
            "link": "metrs_poem.php?poem=19957"
        },
        {
            "name": "\"Вийти — ходити. Ввійти — і ходити знов...\"",
            "link": "metrs_poem.php?poem=19956"
        },
        {
            "name": "\"Завтра випаде дорога...\"",
            "link": "metrs_poem.php?poem=11170"
        },
        {
            "name": "\"Кіт на глині...\"",
            "link": "metrs_poem.php?poem=19959"
        },
        {
            "name": "\"Ми за руки візьмемось на людях...\"",
            "link": "metrs_poem.php?poem=20089"
        },
        {
            "name": "\"Не тільки ілюзій...\"",
            "link": "metrs_poem.php?poem=19953"
        },
        {
            "name": "\"Недивлячись на те, що я сказав колись...\"",
            "link": "metrs_poem.php?poem=11165"
        },
        {
            "name": "\"Отче наш...\"",
            "link": "metrs_poem.php?poem=19954"
        },
        {
            "name": "\"Пізній вечір...\"",
            "link": "metrs_poem.php?poem=19960"
        },
        {
            "name": "\"Я не знаю...\"",
            "link": "metrs_poem.php?poem=19958"
        },
        {
            "name": "\"а з тебе вийшла би непогана черниця...\"",
            "link": "metrs_poem.php?poem=11167"
        },
        {
            "name": "\"востаннє твій хворий на голову голос почути...\"",
            "link": "metrs_poem.php?poem=11168"
        },
        {
            "name": "\"ми візьмемо трохи вина...\"",
            "link": "metrs_poem.php?poem=11171"
        },
        {
            "name": "\"ніби два крила на щоки...\"",
            "link": "metrs_poem.php?poem=11166"
        },
        {
            "name": "\"ось весна. що робити з нею?..\"",
            "link": "metrs_poem.php?poem=20091"
        },
        {
            "name": "Із циклу “ЗГОРІЛА КОРЧМА”",
            "link": "metrs_poem.php?poem=20090"
        },
        {
            "name": "Від’їзд. Парк Шевченка",
            "link": "metrs_poem.php?poem=20088"
        },
        {
            "name": "МОЙСЕЙ ",
            "link": "metrs_poem.php?poem=19955"
        }
    ]
