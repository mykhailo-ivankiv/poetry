{
    "author": "Лятуринська Оксана",
    "link": "/metrs.php?id=324&type=tvorch",
    "poems": [
        {
            "name": "\"І знову цілу ніч верзлося...\"",
            "link": "metrs_poem.php?poem=8036"
        },
        {
            "name": "\"Важкі киреї, золоті ...\"",
            "link": "metrs_poem.php?poem=8041"
        },
        {
            "name": "\"Встелю килимом ослін...\"",
            "link": "metrs_poem.php?poem=8029"
        },
        {
            "name": "\"Давно затерся слід копит...\"",
            "link": "metrs_poem.php?poem=8030"
        },
        {
            "name": "\"За око — враже око...\"",
            "link": "metrs_poem.php?poem=8038"
        },
        {
            "name": "\"Зуб, ратище, копито, пазур...\"",
            "link": "metrs_poem.php?poem=8033"
        },
        {
            "name": "\"Ліси ловитв! Опійні ночі!\"",
            "link": "metrs_poem.php?poem=8034"
        },
        {
            "name": "\"На озера на глибокі...\"",
            "link": "metrs_poem.php?poem=8044"
        },
        {
            "name": "\"Не знаю, як це почалось...\"",
            "link": "metrs_poem.php?poem=8043"
        },
        {
            "name": "\"Один лишився сам на сам...\"",
            "link": "metrs_poem.php?poem=8037"
        },
        {
            "name": "\"Створив ти землю, оболоки...\"",
            "link": "metrs_poem.php?poem=8039"
        },
        {
            "name": "\"Ти ще не вмер, ти ще не вмер!\"",
            "link": "metrs_poem.php?poem=8040"
        },
        {
            "name": "\"Устами славлена ...\"",
            "link": "metrs_poem.php?poem=8035"
        },
        {
            "name": "\"Хилились стязі, пнулись вгору...\"",
            "link": "metrs_poem.php?poem=8031"
        },
        {
            "name": "\"Щастя згублені ключі...\"",
            "link": "metrs_poem.php?poem=8032"
        },
        {
            "name": "ВОЛИНСЬКІ БУКОЛІКИ",
            "link": "metrs_poem.php?poem=8042"
        },
        {
            "name": "НА ТРУНУ ОЛЬЖИЧЕВІ",
            "link": "metrs_poem.php?poem=8045"
        }
    ]
}