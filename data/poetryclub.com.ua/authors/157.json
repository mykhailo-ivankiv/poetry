    "poems": [
        {
            "name": "\"Є в осені невидима межа...\"",
            "link": "metrs_poem.php?poem=371"
        },
        {
            "name": "\"Ах, як вона цвіла, чарівна квітка...\"",
            "link": "metrs_poem.php?poem=13368"
        },
        {
            "name": "\"Благословиться хай довіку...\"",
            "link": "metrs_poem.php?poem=13306"
        },
        {
            "name": "\"Відцокотів я пів-Європи...\"",
            "link": "metrs_poem.php?poem=13288"
        },
        {
            "name": "\"Викочується із пам'яті...\"",
            "link": "metrs_poem.php?poem=18864"
        },
        {
            "name": "\"Вона жива, любов моя єдина...\"",
            "link": "metrs_poem.php?poem=13320"
        },
        {
            "name": "\"Гармоніст Михалко Горобець...\"",
            "link": "metrs_poem.php?poem=13255"
        },
        {
            "name": "\"Дві зорі цвіло у небі...\"",
            "link": "metrs_poem.php?poem=13270"
        },
        {
            "name": "\"Де ти, друже мій єдиний...\"",
            "link": "metrs_poem.php?poem=13363"
        },
        {
            "name": "\"До яблуні навшпиньки підійшла...\"",
            "link": "metrs_poem.php?poem=13394"
        },
        {
            "name": "\"Зима... Нема їй ні кінця, ні краю...\"",
            "link": "metrs_poem.php?poem=13415"
        },
        {
            "name": "\"Знаєш, я частенько вже боюсь...\"",
            "link": "metrs_poem.php?poem=13387"
        },
        {
            "name": "\"Зозуля кувала серед ночі...\"",
            "link": "metrs_poem.php?poem=13418"
        },
        {
            "name": "\"Коли мене на цій землі не стане...\"",
            "link": "metrs_poem.php?poem=13450"
        },
        {
            "name": "\"Коли спадає день шалений...\"",
            "link": "metrs_poem.php?poem=13314"
        },
        {
            "name": "\"Мені ти маму замінила...\"",
            "link": "metrs_poem.php?poem=13518"
        },
        {
            "name": "\"Мене раптово будять поїзди...\"",
            "link": "metrs_poem.php?poem=13279"
        },
        {
            "name": "\"Ми з ним лежали в одній палаті...\"",
            "link": "metrs_poem.php?poem=13244"
        },
        {
            "name": "\"Монтаж - це мука...\"",
            "link": "metrs_poem.php?poem=13360"
        },
        {
            "name": "\"Моя прекрасна, горда королево...\"",
            "link": "metrs_poem.php?poem=13305"
        },
        {
            "name": "\"На вулиці - облич рухливі зорі...\"",
            "link": "metrs_poem.php?poem=13252"
        },
        {
            "name": "\"На сполох б'є душа моя хоробра...\"",
            "link": "metrs_poem.php?poem=13440"
        },
        {
            "name": "\"Не пишні словечка фальшиві...\"",
            "link": "metrs_poem.php?poem=13376"
        },
        {
            "name": "\"Не подарую, не прощу...\"",
            "link": "metrs_poem.php?poem=13533"
        },
        {
            "name": "\"Не ховаю у пам'ять нічого...\"",
            "link": "metrs_poem.php?poem=13310"
        },
        {
            "name": "\"Нехай чужих планет не чути...\"",
            "link": "metrs_poem.php?poem=13351"
        },
        {
            "name": "\"Облітає цвіт вишневий...\"",
            "link": "metrs_poem.php?poem=13365"
        },
        {
            "name": "\"Окропило окропом...\"",
            "link": "metrs_poem.php?poem=18860"
        },
        {
            "name": "\"Під ковдрою засніженого вечора...\"",
            "link": "metrs_poem.php?poem=13295"
        },
        {
            "name": "\"Під серце - словами, словами...\"",
            "link": "metrs_poem.php?poem=13564"
        },
        {
            "name": "\"Перегорю...\"",
            "link": "metrs_poem.php?poem=13388"
        },
        {
            "name": "\"По зелених маршрутах весни...\"",
            "link": "metrs_poem.php?poem=13249"
        },
        {
            "name": "\"Помирають батьки, як солдати...\"",
            "link": "metrs_poem.php?poem=13373"
        },
        {
            "name": "\"Ромашко моя вечорова...\"",
            "link": "metrs_poem.php?poem=13321"
        },
        {
            "name": "\"Рости, мій сину...\"",
            "link": "metrs_poem.php?poem=13258"
        },
        {
            "name": "\"Світ мені усміхається тепло....\"",
            "link": "metrs_poem.php?poem=13328"
        },
        {
            "name": "\"Світи мені, кохана...\"",
            "link": "metrs_poem.php?poem=13392"
        },
        {
            "name": "\"Так мені, бува, не вистачає...\"",
            "link": "metrs_poem.php?poem=13457"
        },
        {
            "name": "\"Там, за рівниною голою...\"",
            "link": "metrs_poem.php?poem=18862"
        },
        {
            "name": "\"Тато косить конюшину...\"",
            "link": "metrs_poem.php?poem=18863"
        },
        {
            "name": "\"Титани! Ось він перед вами...\"",
            "link": "metrs_poem.php?poem=13551"
        },
        {
            "name": "\"У вічність ідуть ветерани війни...\"",
            "link": "metrs_poem.php?poem=13407"
        },
        {
            "name": "\"Україно! Пісня наша лине...\"",
            "link": "metrs_poem.php?poem=13402"
        },
        {
            "name": "\"Хто ти, хто ти, Незнайомко...\"",
            "link": "metrs_poem.php?poem=13319"
        },
        {
            "name": "\"Чи зросту зерном у пісні щирій...\"",
            "link": "metrs_poem.php?poem=13421"
        },
        {
            "name": "\"Чому себе я так неволю...\"",
            "link": "metrs_poem.php?poem=13393"
        },
        {
            "name": "\"Чому...\"",
            "link": "metrs_poem.php?poem=13293"
        },
        {
            "name": "\"Чорні очі кари...\"",
            "link": "metrs_poem.php?poem=18861"
        },
        {
            "name": "\"Я народився в переджнив'я...\"",
            "link": "metrs_poem.php?poem=13280"
        },
        {
            "name": "\"Я немов занедужав...\"",
            "link": "metrs_poem.php?poem=13563"
        },
        {
            "name": "\"Я прийшов у світ цей ненадовго...\"",
            "link": "metrs_poem.php?poem=13398"
        },
        {
            "name": "\"Я спитав у води...\"",
            "link": "metrs_poem.php?poem=13469"
